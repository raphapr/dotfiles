var MAX_SIZE = 100;

// Use native regex replacement instead of split/join
var newlineRegex = /\\n/g;

// Pre-build format for faster concatenation
var results = [];

for (var i = 0; i < MAX_SIZE; i++) {
    var item = str(read(i));
    // Use regex replace (faster than split/join)
    var processedItem = item.replace(newlineRegex, "(newline)");
    results.push("(" + i + ") " + processedItem + "\\n");
}

// Single print call instead of multiple
print(results.join(""));
