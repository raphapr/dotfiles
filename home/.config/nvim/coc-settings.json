{
  "suggest.noselect": false,
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "?",
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "yml",
    "yaml",
    "json",
    "css",
    "scss",
    "python",
    "tf",
    "go"
  ],
  "go.goplsArgs": [
    "-remote",
    "unix;/tmp/gopls-daemon-socket"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "filetypes": [
        "go"
      ]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {}
    }
  }
}
